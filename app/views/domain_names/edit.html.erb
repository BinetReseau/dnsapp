<h1><%= @title %></h1>

<%= form_for(@domain_name) do |f| %>
  <%= render 'fields', :f => f %>
  <div class="actions">
    <%= f.submit "Modifier" %>
  </div>
<% end %>

<p>
  <%= link_to "Retour au profil de #{@domain_name.get_short_name}", @domain_name %> | 
  <%= link_to "Liste des DNS", domain_names_path %>
</p>
